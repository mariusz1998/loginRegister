<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/CSS/style.css">
    <title>Select attribute</title>
 </head>
<script>var toInit = "Attr";</script>
<body onload="init(toInit);">
<center>
       Select file property:          
        <br/><br/>
        <div id="container">
            <div id="avaible">
               Avaible: 
                <br/>
                <iframe name="AvaibleAttr" id="AvaibleAttr" src="/attr/avaible"></iframe>
            </div>
            <div id="added">
                Selected:
                <br/>
                <iframe id="ChoosedAttr" src="/attr/choosed"></iframe>
            </div>
        </div>
        <br></br>
        <div>
            <label for="newAttribute">Add attribute</label>
            <input type="text" id="newAttribute" name="newAttribute" >
            <input type = "submit" value = "Add attribute" onclick="addAttr()">
          </div>
        <div>
            <label for="localization">Localization</label>
            <input type="text" id="localization" name="localization" >
          </div>
          <div>
            <label for="firstDay">First day:</label>
          <input type="date" id="firstDay" name="firstDay" onchange="firstDayChange()" > 
        </div>
          <div>
            <label for="lastDay">Last day:</label>
          <input type="date" id="lastDay" name="lastDay" onchange="lastDayChange()" >
        </div>
        <input type = "submit" value = "Add file" onclick="temp()">
        <br/><br/>
      
</center>
<script>
      function addAttr()
    {
        addNewAttr('Attr');
    }
    function temp()
    {
        submit();
    }
    function firstDayChange(){
     document.getElementById("lastDay").setAttribute("min", document.getElementById("firstDay").value);
    }
    function lastDayChange(){
        document.getElementById("firstDay").setAttribute("max", document.getElementById("lastDay").value);
    }
</script>
<script type="text/javascript" src="/forLists/initFunction.js"></script>
<script type="text/javascript" src="/forLists/addFunction.js"></script>
<script type="text/javascript" src="/forLists/deleteFunction.js"></script>
<script type="text/javascript" src="/forLists/optionsCounter.js"></script>
<script type="text/javascript" src="/submits/addAttribute.js"></script>
<script type="text/javascript" src="/submits/sendFileAttribute.js"></script>
</body>

</html>


