    <link rel="stylesheet" href="/CSS/style.css">
    <title>Select users</title>
 </head>
<script>
    var arrayUsersFile =[] 
    console.log('<%- users%>')
    '<% for(var j=0; j<users.length; j++) { %>'
        arrayUsersFile.push('<%- users[j]%>');
       ' <%}%>'
var toInit = "Users";</script>
<body  onload="init(toInit,arrayUsersFile);">
